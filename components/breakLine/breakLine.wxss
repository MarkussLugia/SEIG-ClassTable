/* components/cardFrame/cardFrame.wxss */

:host {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: stretch;
}

.text {
  font-size: 32rpx;
  white-space: nowrap;
  flex-shrink: 0;
  flex-grow: 0;
  margin-right: 10rpx;
  color: #9A9A9A
}

.hrBreakLine {
  flex-grow: 1;
  height: 0;
  border-top: 4rpx dotted #9A9A9A;
  margin-top: 4rpx;
}