/* pages/fullTable/fullTable.wxss */
.head{
  position: fixed;
  z-index: 256;
  top:0;
  width: 100%;
  height: 170rpx;
}
.tableHead{
  position: absolute;
  box-sizing: border-box;
  text-align: center;
  width: 100%;
  height: 54rpx;
  bottom: 8rpx;
  flex-flow: row nowrap;
  align-items: stretch;
  justify-content: space-between;
  padding: 0 10rpx;
}
.headItem{
  flex-grow: 0;
  flex-shrink: 0;
  width: calc(20% - 8rpx);
  height:100%
}
.classes{
  z-index: 0;
}
.table{
  margin-top: 160rpx;
  padding-bottom: 12rpx;
  margin-bottom: 100rpx;
}
.primeRow{
  box-sizing: border-box;
  margin: 8rpx 0 0;
  padding: 0;
}
.tableRow{
  flex-flow: row nowrap;
  align-items: stretch;
  justify-content: space-between;
  padding: 0 10rpx;
}
.time{
  box-sizing: border-box;
  padding-left: 12rpx;
  font-weight: bold;
  line-height: 64rpx;
  font-size: 36rpx;
}